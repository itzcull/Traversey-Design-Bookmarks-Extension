{
  "manifest_version": 3,
  "name": "Traversey Design Sync",
  "description": "Chrome extension for syncing Traversey Media design resources repo to bookmarks",
  "version": "0.0.1",
  "icons": {
    "128": "assets/trav_logo.png"
  },
  "options_page": "options.html",
  "action": {
    "default_icon": {
      "128": "assets/trav_logo.png"
    }
  },
  "permissions": ["bookmarks"],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  }
}
